<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiilab.mapper2.AmbariMapper">
    
    <select id="getMetricList" resultType="hashmap">
    	SELECT 
    		*
    	FROM 
    		metric_registry
    	WHERE
    		1=1
    </select>
    
    
</mapper>
