<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>
<configuration supports_final="false">
    <property>
        <name>ldap_manager_name</name>
        <display-name>OpenLDAP manager name</display-name>
        <value>Manager</value>       
        <description>Set OpenLDAP manager name.</description>
    </property>
    <property>
        <name>ldap_manager_pass</name>
        <display-name>OpenLDAP manager password</display-name>
        <value></value>       
        <description>Set OpenLDAP manager password.</description>
    </property>
    <property>
        <name>ldap_domain</name>
        <display-name>OpenLDAP domain set</display-name>
        <value>dc=srv,dc=world</value>       
        <description>Set OpenLDAP admin password.</description>
    </property>
</configuration>